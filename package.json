{
  "name": "solid-material-ui",
  "version": "0.1.0",
  "main": "lib/index.js",
  "description": "Material like ui library using SolidJS.",
  "repository": "git@github.com:azuwey/solid-material-ui.git",
  "author": "David Zarandi <david.zarandi@pm.me>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-solid": "^2.2.5"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "solid-js": "^1.3.7"
  },
  "scripts": {
    "clean:package": "rm -rf lib",
    "lint:package": "ts-standard -p src/tsconfig.json src/ --fix",
    "prebuild:package": "npm lint:package && npm clean:package",
    "build:package": "tsc -p src/tsconfig.json",
    "dev:docs": "vite docs"
  },
  "files": [
    "/lib"
  ],
  "ts-standard": {
    "ignore": [
      "node_modules/*",
      "lib/**/*"
    ]
  },
  "keywords": [
    "material",
    "like",
    "ui",
    "solidjs",
    "solid-js",
    "solid"
  ]
